  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/x-charts": "^8.13.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@tkomde/iohook": "^1.1.6",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "active-win": "^8.2.1",
    "input-tracker": "^1.0.12",
    "lowdb": "^7.0.1",
    "onnxruntime-node": "^1.23.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.3",
    "react-router-dom": "^7.9.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "electron": "^38.2.1",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"electron ./electron\"",
    "build": "react-scripts build && electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "iohook": {
    "targets": [
      "node-127",
      "electron-139"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64",
      "ia32"
    ]
  },
  "electron": "electron .",
  "dist": "electron-builder",
  "build": {
    "appId": "com.rockhyrax.destressify",
    "productName": "DeStressify",
    "directories": {
      "output": "dist"
    }
  }
}
